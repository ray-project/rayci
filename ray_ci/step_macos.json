{
	"plugins": [{
		"ray-project/macos-sandbox#v1.0.7": {
			"deny-file-read": "/usr/local/etc/buildkite-agent/buildkite-agent.cfg"
		}
	}],
	"agents": {
		"queue": "!!REPLACE!!"
	},
	"timeout_in_minutes": 180,
	"artifact_paths": "/tmp/artifacts/.*/**",
    "env": {"PS4": "> "},
	"retry": {
		"manual": {
			"permit_on_passed": true
		},
		"automatic": {
			"exit_status": -1,
			"limit": 3
		}
	},
	"notify": ["github_commit_status"]
}